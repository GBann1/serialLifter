{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "main-capability",
    "description": "Capability for the main window",
    "windows": ["main"],
    "permissions": [
      "path:default",
      "event:allow-emit",
      "event:allow-listen",
      "window:default",
      "app:default",
      "resources:default",
      "menu:default",
      "tray:default",
      "window:allow-set-title",
      "webview:allow-internal-toggle-devtools"
    ]
  }